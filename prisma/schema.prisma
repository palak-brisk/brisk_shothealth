  RequestShotServices  RequestShotServices? @relation(fields: [RequestShotServiceId], references: [Id], onDelete: NoAction, onUpdate: NoAction, map: "FK_UserWallet_RequestShotServices_RequestShotServiceId")
  WithdrawRequest      WithdrawRequest?     @relation(fields: [WithdrawRequestId], references: [Id], onDelete: NoAction, onUpdate: NoAction, map: "FK_UserWallet_WithdrawRequest_WithdrawRequestId")

  @@index([RequestShotServiceId], map: "IX_UserWallet_RequestShotServiceId")
  @@index([UserId], map: "IX_UserWallet_UserId")
  @@index([WithdrawRequestId], map: "IX_UserWallet_WithdrawRequestId")
}

model WithdrawRequest {
  Id                   String           @id(map: "PK_WithdrawRequest") @db.UniqueIdentifier
  UserId               BigInt
  WithdrawAmount       Decimal          @db.Decimal(18, 2)
  Status               Int
  TransactionNotes     String?          @db.NVarChar(Max)
  CreatorUserId        BigInt?
  CreationTime         DateTime
  LastModifierUserId   BigInt?
  LastModificationTime DateTime?
  DeleterUserId        BigInt?
  DeletionTime         DateTime?
  IsDeleted            Boolean
  TenantId             Int?
  AttachmentId         String?          @db.UniqueIdentifier
  TransactionTime      DateTime?
  BankAccountId        String?          @db.UniqueIdentifier
  UserWallet           UserWallet[]
  AbpUsers             AbpUsers         @relation(fields: [UserId], references: [Id], onDelete: Cascade, onUpdate: NoAction, map: "FK_WithdrawRequest_AbpUsers_UserId")
  Attachments          Attachments?     @relation(fields: [AttachmentId], references: [Id], onDelete: NoAction, onUpdate: NoAction, map: "FK_WithdrawRequest_Attachments_AttachmentId")
  UserBankDetails      UserBankDetails? @relation(fields: [BankAccountId], references: [Id], onDelete: NoAction, onUpdate: NoAction, map: "FK_WithdrawRequest_UserBankDetails_BankAccountId")

  @@index([AttachmentId], map: "IX_WithdrawRequest_AttachmentId")
  @@index([BankAccountId], map: "IX_WithdrawRequest_BankAccountId")
  @@index([UserId], map: "IX_WithdrawRequest_UserId")
}
